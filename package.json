{
    "name": "vibe-code-starter",
    "version": "1.1.13",
    "description": "A cli tool to start a vibe coding project",
    "main": "./dist/index.js",
    "type": "module",
    "scripts": {
        "test": "npm run build && cd ../ && node ./vibe-code-starter/dist/index.js && cursor .",
        "local": "node ./dist/index.js",
        "prebuild": "node -p \"'export default ' + JSON.stringify(require('./package.json').version) + '; ' + '/* DO NOT EDIT, THIS FILE IS AUTOMATICALLY GENERATED */'\" > src/version.ts",
        "build": "rm -rf ./dist && tsc -p tsconfig.json"
    },
    "author": "Niels van Rijn",
    "license": "ISC",
    "bin": {
        "vibe-code-starter": "./dist/index.js"
    },
    "engines": {
        "node": ">=18"
    },
    "dependencies": {
        "gradient-string": "^3.0.0",
        "kolorist": "^1.8.0",
        "minimist": "^1.2.8",
        "ora": "^8.1.1",
        "prompts": "^2.4.2",
        "shelljs": "^0.8.5"
    },
    "devDependencies": {
        "@tsconfig/node-lts": "^22.0.0",
        "@types/gradient-string": "^1.1.6",
        "@types/minimist": "^1.2.5",
        "@types/node": "^22.9.0",
        "@types/prompts": "^2.4.9",
        "@types/shelljs": "^0.8.15",
        "prettier": "3.3.3",
        "typescript": "^5.6.3"
    }
}
